<% cache @article.cache_key %>

    <p id="notice"><%= notice %></p>

    <p>
      <strong>Title:</strong>
      <%= @article.title %>
    </p>

    <p>
      <strong>Content:</strong>
      <%= @article.content %>
    </p>

    <p>
      <strong>Metadata:</strong>
      <%= @article.metadata %>
    </p>

    <%= link_to 'Edit', edit_article_path(@article) %> |
    <%= link_to 'Back', articles_path %>

<% end %>